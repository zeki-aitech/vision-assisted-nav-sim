export TURTLEBOT3_MODEL=waffle_pi
ros2 launch turtlebot3_gazebo turtlebot3_world.launch.py

export TURTLEBOT3_MODEL=waffle_pi
ros2 launch turtlebot3_navigation2 navigation2.launch.py use_sim_time:=True

# use this to build package and its dependencies
colcon build --packages-up-to yolo_ros

ros2 launch vision_assisted_nav_sim tbot_gazebo_sim.launch.py
ros2 launch tbot_nav2 nav2.launch.py use_sim_time:=true
ros2 launch yolo_bringup yolo.launch.py model:=yolov8n-seg.pt input_image_topic:=/camera/image_raw

ros2 launch tbot_gazebo sim.launch.py x_pose:=-5.0 y_pose:=-2.5